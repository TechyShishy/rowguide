{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./projects/rowguide/src/app/"],
  "entryPointStrategy": "expand",
  "out": "../..//docs/api/",
  "exclude": ["**/*.spec.ts", "**/*.spec.js", "**/node_modules/**"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "../../README.md",
  "name": "Rowguide API Documentation",
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core Services",
    "Data Services",
    "Feature Services",
    "Components",
    "Models",
    "Interfaces",
    "State Management",
    "Utilities",
    "*"
  ],
  "githubPages": false,
  "hideGenerator": true,
  "searchInComments": true,
  "cleanOutputDir": true
}
