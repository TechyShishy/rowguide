{
  "name": "rowguide",
  "version": "0.6.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "package": "tools/inject-scripts.js",
    "watch": "ng build --watch --configuration development",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "ng test --watch=false --browsers=ChromeHeadless",
    "test:unit:headed": "ng test --watch=false --browsers=Chrome",
    "test:e2e": "playwright test --reporter=line",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "docs:build": "node scripts/build-docs.js",
    "docs:clean": "rm -rf ../../docs/api",
    "docs:dev": "node scripts/deploy-docs.js local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/cdk": "20.0.5",
    "@angular/common": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/material": "20.0.5",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "idb": "^8.0.0",
    "js-file-download": "^0.4.12",
    "log4js": "^6.9.1",
    "ngx-logger": "^5.0.12",
    "pako": "^2.1.0",
    "pdfjs-dist": "^4.10.38",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "packageManager": "yarn@4.9.2",
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler": "^20.0.6",
    "@angular/compiler-cli": "^20.0.6",
    "@playwright/test": "^1.54.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.11.25",
    "@types/pako": "^2",
    "angular-file": "^4.0.2",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typedoc": "^0.28.7",
    "typescript": "~5.8.3"
  }
}
