{
  "name": "rowguide",
  "version": "0.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "package": "tools/inject-scripts.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:generate": "typedoc",
    "docs:serve": "http-server ../../docs/api -p 8080 -o",
    "docs:build": "node scripts/build-docs.js",
    "docs:clean": "rm -rf ../../docs/api",
    "docs:validate": "typedoc --validation.notExported --validation.invalidLink --validation.notDocumented --logLevel Warn",
    "docs:coverage": "typedoc --listInvalidSymbolLinks --logLevel Error",
    "docs:dev": "node scripts/deploy-docs.js local",
    "docs:deploy": "node scripts/deploy-docs.js",
    "docs:deploy:github": "node scripts/deploy-docs.js github",
    "docs:deploy:local": "node scripts/deploy-docs.js local",
    "docs:watch": "node scripts/deploy-docs.js watch",
    "docs:validate:structure": "node scripts/deploy-docs.js validate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/cdk": "20.0.5",
    "@angular/common": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/material": "20.0.5",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "idb": "^8.0.0",
    "js-file-download": "^0.4.12",
    "log4js": "^6.9.1",
    "ngx-logger": "^5.0.12",
    "pako": "^2.1.0",
    "pdfjs-dist": "^4.10.38",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "packageManager": "yarn@4.1.0+sha256.81a00df816059803e6b5148acf03ce313cad36b7f6e5af6efa040a15981a6ffb",
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler": "^20.0.6",
    "@angular/compiler-cli": "^20.0.6",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.11.25",
    "@types/pako": "^2",
    "angular-file": "^4.0.2",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typedoc": "^0.28.7",
    "typescript": "~5.8.3"
  }
}
